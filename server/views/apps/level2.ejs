<!DOCTYPE html>
<html lang="en">
<head>
  <% include ../session.ejs%>
  <% include ../head.ejs%>
</head>
<body>
<div class=”main-container”>
<% include header.ejs%>
<div id="container" style="max-width: 720px;margin: 0 auto;width: 100%;padding-top: 10px;z-index: -1">
  <div class="text-center" style="color:#003366;font-size:20px;padding-bottom:0px;">
    <div class="p-1">
      <p><strong><%=level1 %></strong></p>
    </div>
  </div>
<% var len = data.length;
  for(var index=1; index<=len; index++) {
%>
  <div class="row" style="padding: 15px; background-color: white;">
    <div class="col-6">
      <a href="apps/level3?i=0&j=1&level1=<%=level1 %>&level2=<%=data[index-1] %>">
        <div class="text-center" style="padding-bottom: 10px;">
          <img width="32" src='images/<%=data[index-1] %>.png'></img>
        </div>
        <div class="text-center">
          <span style="color:#003366;font-size:16px;">
            <strong><%=data[index-1] %></strong>
          </span>
        </div>
      </a>
    </div>
    <div class="col-6">
      <% if(data[index]) { %>
      <a href="apps/level3?i=1&j=2&level1=<%=level1 %>&level2=<%=data[index] %>">
        <div class="text-center" style="padding-bottom: 10px;">
          <img width="32" src='images/<%=data[index] %>.png'></img>
        </div>
        <div class="text-center">
          <span style="color:#003366;font-size:16px;">
            <strong><%=data[index] %></strong>
          </span>
        </div>
      </a>
      <% } %>
    </div>
  </div>
<% index ++;} %>
</div>
<footer>
  <% include ./menu.ejs%>
</footer>
</div>
</body>
</html>
